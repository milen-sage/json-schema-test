{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "human",
  "title": "human",
  "type": "object",
  "properties": {
    "sex": {
      "enum": [
        "M",
        "Other",
        "F",
        ""
      ]
    },
    "ethnicity": {
      "not": {
        "type": "null"
      },
      "minLength": 1
    },
    "age": {
      "not": {
        "type": "null"
      },
      "minLength": 1
    }
  },
  "required": [
    "ethnicity",
    "age"
  ]
}
