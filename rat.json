{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "rat",
  "title": "rat",
  "type": "object",
  "properties": {
    "ratStrain": {
      "enum": [
        "Brattleboro",
        "Sprague Dawley",
        "Lewis",
        "Long-Evans"
      ]
    },
    "rodentCoatColor": {
      "enum": [
        "brown",
        "gray",
        "white",
        ""
      ]
    },
    "age": {
      "not": {
        "type": "null"
      },
      "minLength": 1
    }
  },
  "required": [
    "ratStrain",
    "age"
  ]
}
